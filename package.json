{
  "name": "office-js-snippets",
  "version": "1.0.0",
  "description": "A collection of samples using the Office JavaScript API",
  "main": "config/compile.js",
  "engines": {
    "node": ">=6.10.0"
  },
  "scripts": {
    "start": "npm run tsc && npm run build && npm run lint",
    "tsc": "tsc -p tsconfig.json",
    "build": "node config/build.js",
    "lint": "tslint --project tsconfig.json",
    "test": "jest",
    "test:compile": "jest snippet-compiler",
    "test:libs": "jest library-validator",
    "test:runtime": "jest runtime-execution --testPathIgnorePatterns=refactored --testPathIgnorePatterns=expanded --testPathIgnorePatterns=auto-generated",
    "test:runtime:all": "jest runtime-execution",
    "test:runtime:auto": "jest runtime-auto-generated",
    "test:runtime:expanded": "jest runtime-execution-expanded",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:report": "ts-node --project tsconfig.test.json tests/generate-failure-report.ts",
    "validate": "npm run tsc && npm run build && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OfficeDev/office-js-snippets.git"
  },
  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OfficeDev/office-js-snippets/issues"
  },
  "homepage": "https://github.com/OfficeDev/office-js-snippets#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "escape-string-regexp": "^4.0.0",
    "exceljs": "^4.4.0",
    "fs-extra": "11.3.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "rimraf": "^6.0.1",
    "shelljs": "^0.10.0"
  },
  "devDependencies": {
    "@types/custom-functions-runtime": "^1.6.11",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.10",
    "@types/node": "^24.5.2",
    "@types/office-js": "^1.0.569",
    "@types/office-js-preview": "^1.0.679",
    "@types/shelljs": "^0.8.17",
    "jest": "^29.7.0",
    "office-addin-mock": "^3.0.6",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.9.2"
  }
}
